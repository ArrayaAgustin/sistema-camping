{
  "name": "camping-smata-api",
  "version": "1.0.0",
  "description": "API SMATA - Afiliados y gesti√≥n de campings (Prisma + Express)",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register dist/server.js",
    "start:js": "node src/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "dev:js": "nodemon src/server.js",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=src/models/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/models/prisma/schema.prisma",
    "test": "node tests/verify-admin.js && node tests/test-login-route.js && node tests/test-afiliados.js",
    "test:auth": "node tests/test-login-route.js",
    "test:afiliados": "node tests/test-afiliados.js",
    "verify": "node tests/verify-admin.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "prisma": "^5.10.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/types": "dist/types",
    "@/interfaces": "dist/interfaces",
    "@/config": "dist/config",
    "@/utils": "dist/utils",
    "@/services": "dist/services",
    "@/controllers": "dist/controllers",
    "@/middlewares": "dist/middlewares",
    "@/routes": "dist/routes"
  }
}
